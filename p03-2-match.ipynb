    "def find_match(n):\n",
    "    a = len(n)\n",
    "    for i in range(0, a-1):\n",
    "        for j in range(i+1, a):\n",
    "            print('%s - %s'%(n[i], n[j]))\n",
    "name = [\"Tom\", \"Jerry\", \"Mike\"]\n",
    "find_match(name)"
